<mvc:View controllerName="ui5lab.app.GeometryApp.controller.App"
	xmlns:mvc="sap.ui.core.mvc"
	xmlns:core="sap.ui.core"
	xmlns:fb="sap.ui.comp.filterbar"
	xmlns:f="sap.f"
	xmlns="sap.m">
	<f:DynamicPage headerExpanded="true" class="sapUiSizeCompact">
		<f:header>
			<f:DynamicPageHeader pinnable="true">
				<f:content>
					<fb:FilterBar reset="onReset" search="onSearch" useToolbar="false" showGoOnFB="false" showFilterConfiguration="false" filterBarExpanded="true" filterContainerWidth="10rem">
						<fb:filterItems>
							<fb:FilterItem name="MultiComboBoxFilter" label="Test">
								<fb:control>
									<MultiComboBox id="testMultiSelect" >
										<core:Item key="A" text="{i18n>appItemA}" />
										<core:Item key="B" text="{i18n>appItemB}" />
										<core:Item key="C" text="{i18n>appItemC}" />
										<core:Item key="D" text="{i18n>appItemD}" />
										<core:Item key="E" text="{i18n>appItemE}" />
										<core:Item key="F" text="{i18n>appItemF}" />
									</MultiComboBox>
								</fb:control>
							</fb:FilterItem>
						</fb:filterItems>
					</fb:FilterBar>
				</f:content>
			</f:DynamicPageHeader>
		</f:header>
	</f:DynamicPage>

</mvc:View>